{
    "name": "ember-syntax",
    "displayName": "Ember Syntax",
    "description": "Ember HTMLBars and Glimmer syntax highlighting",
    "version": "0.0.1",
    "publisher": "dhedgecock",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "glimmer",
            "aliases": ["Glimmer", "glimmer"],
            "extensions": [".hbs"],
            "configuration": "./language-configuration.json"
        }, {
            "id": "javascriptglimmer",
            "aliases": ["JavaScript (Babel+Glimmer)", "javascriptglimmer"],
            "extensions": [".js"],
            "configuration": "./javascript-language-configuration.json"
        }, {
            "id": "typescriptglimmer",
            "aliases": ["TypeScript (Glimmer)", "typescriptglimmer"],
            "extensions": [".ts"],
            "configuration": "./javascript-language-configuration.json"
        }],
        "grammars": [{
            "language": "glimmer",
            "scopeName": "source.glimmer",
            "path": "./syntaxes/glimmer.tmLanguage.json"
        }, {
            "language": "javascriptglimmer",
            "scopeName": "source.js",
            "path": "./syntaxes/javascriptglimmer.tmLanguage.json"
        }, {
            "language": "typescriptglimmer",
            "scopeName": "source.ts",
            "path": "./syntaxes/typescriptglimmer.tmLanguage.json"
        }]
    }
}